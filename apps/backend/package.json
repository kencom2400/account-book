{
  "name": "@account-book/backend",
  "version": "1.0.0",
  "description": "",
  "author": "",
  "private": true,
  "license": "ISC",
  "scripts": {
    "dev": "nest start --watch",
    "build": "nest build",
    "start": "node dist/main",
    "lint": "eslint \"{src,test}/**/*.ts\" --max-warnings 0",
    "test": "jest",
    "test:unit": "jest",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:e2e:watch": "jest --config ./test/jest-e2e.json --watch",
    "test:e2e:cov": "jest --config ./test/jest-e2e.json --coverage",
    "test:perf": "jest --config ./test/jest-perf.json",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "clean": "rm -rf dist",
    "migration:generate": "ts-node --transpile-only -r tsconfig-paths/register -P tsconfig.json ./node_modules/typeorm/cli.js migration:generate -d src/config/typeorm-migration.config.ts",
    "migration:create": "ts-node --transpile-only -r tsconfig-paths/register -P tsconfig.json ./node_modules/typeorm/cli.js migration:create",
    "migration:run": "ts-node --transpile-only -r tsconfig-paths/register -P tsconfig.json ./node_modules/typeorm/cli.js migration:run -d src/config/typeorm-migration.config.ts",
    "migration:revert": "ts-node --transpile-only -r tsconfig-paths/register -P tsconfig.json ./node_modules/typeorm/cli.js migration:revert -d src/config/typeorm-migration.config.ts",
    "migration:show": "ts-node --transpile-only -r tsconfig-paths/register -P tsconfig.json ./node_modules/typeorm/cli.js migration:show -d src/config/typeorm-migration.config.ts"
  },
  "dependencies": {
    "@account-book/types": "workspace:^",
    "@account-book/utils": "workspace:^",
    "@nestjs/axios": "^4.0.1",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/schedule": "^6.0.1",
    "@nestjs/typeorm": "^11.0.0",
    "axios": "^1.13.2",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "eventemitter2": "^6.4.9",
    "fs-extra": "^11.3.2",
    "mysql2": "^3.15.3",
    "nest-winston": "^1.10.2",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.27",
    "uuid": "^11.0.3",
    "winston": "^3.18.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^5.0.0",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.10.7",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^30.0.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
